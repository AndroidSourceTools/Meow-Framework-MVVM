    <style name="Meow.Dash.Vertical.SecondaryVariant">
        <item name="dash_color">?colorSecondaryVariant</item>
    </style>

    <style name="Meow.Dash.Vertical.OnPrimary">
        <item name="dash_color">?colorOnPrimary</item>
    </style>

    <style name="Meow.Dash.Vertical.OnSecondary">
        <item name="dash_color">?colorOnSecondary</item>
    </style>

    <style name="Meow.Dash.Vertical.OnSurface">
        <item name="dash_color">?colorOnSurface</item>
    </style>

